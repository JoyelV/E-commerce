<section class="content-main" style="padding: 2rem; background-color: #f5f7fa;">
    <main class="main my-5 mt-2">
        <div class="d-column d-lg-flex justify-content-center m-0">
            <div class="col-lg-6 d-flex flex-column flex-sm-row justify-content-around mt-3">
                <div id="usersList" class="col-sm-5 d-flex flex-column justify-content-center align-items-center" style="
                    border-radius: 12px;
                    height: 120px;
                    width: 200px;
                    background-color: #e8f0fe;
                    box-shadow: 0 4px 8px rgba(0,0,0,0.1);
                    transition: transform 0.2s;
                " onmouseover="this.style.transform='scale(1.05)'" onmouseout="this.style.transform='scale(1)'">
                    <div style="font-size: 2rem; color: #1a73e8; font-weight: 700;"><%- usersCount %></div>
                    <div class="text-center mt-3" style="font-size: 1rem; color: #34495e; font-weight: 500;">Total Users</div>
                </div>
                <div id="productsList" class="col-sm-5 d-flex flex-column justify-content-center align-items-center" style="
                    border-radius: 12px;
                    height: 120px;
                    width: 200px;
                    background-color: #fef7e8;
                    box-shadow: 0 4px 8px rgba(0,0,0,0.1);
                    transition: transform 0.2s;
                " onmouseover="this.style.transform='scale(1.05)'" onmouseout="this.style.transform='scale(1)'">
                    <div style="font-size: 2rem; color: #f39c12; font-weight: 700;"><%- productsCount %></div>
                    <div class="text-center mt-3" style="font-size: 1rem; color: #34495e; font-weight: 500;">Total Products</div>
                </div>
            </div>
            <div class="col-lg-6 d-flex flex-column flex-sm-row justify-content-around mt-3">
                <div id="ordersList" class="col-sm-5 d-flex flex-column justify-content-center align-items-center" style="
                    border-radius: 12px;
                    height: 120px;
                    width: 200px;
                    background-color: #e6f9ef;
                    box-shadow: 0 4px 8px rgba(0,0,0,0.1);
                    transition: transform 0.2s;
                " onmouseover="this.style.transform='scale(1.05)'" onmouseout="this.style.transform='scale(1)'">
                    <div style="font-size: 2rem; color: #27ae60; font-weight: 700;"><%- ordersCount %></div>
                    <div class="text-center mt-3" style="font-size: 1rem; color: #34495e; font-weight: 500;">Total Orders</div>
                </div>
                <div id="salesList" class="col-sm-5 d-flex flex-column justify-content-center align-items-center" style="
                    border-radius: 12px;
                    height: 120px;
                    width : 200px;
                    background-color: #e8f4ff;
                    box-shadow: 0 4px 8px rgba(0,0,0,0.1);
                    transition: transform 0.2s;
                " onmouseover="this.style.transform='scale(1.05)'" onmouseout="this.style.transform='scale(1)'">
                    <div style="font-size: 2rem; color: #2980b9; font-weight: 700;">₹<%- totalRevenue %></div>
                    <div class="text-center mt-3" style="font-size: 1rem; color: #34495e; font-weight: 500;">Total Revenue</div>
                </div>
            </div>
        </div><br><br>
        <a href="/admin/dashboard" class="btn btn-secondary m-3" style="background-color: #3498db; border: none; padding: 0.75rem 1.5rem; border-radius: 8px; color: #ffffff; transition: background-color 0.3s;" onmouseover="this.style.backgroundColor='#2980b9'" onmouseout="this.style.backgroundColor='#3498db'">Chart Page</a>
        <!-- Best Selling -->
        <div class="container text-capitalize d-flex flex-column">
            <div class="row m-3 my-5">
                <h4 class="text-start" style="font-size: 1.5rem; color: #2c3e50; font-weight: 600; margin-bottom: 1.5rem;">Best Selling Products (Top 10)</h4>
                <div class="table-responsive">
                    <table class="table table-hover" style="background-color: #ffffff; border-radius: 8px; overflow: hidden; box-shadow: 0 4px 8px rgba(0,0,0,0.1);">
                        <thead style="background-color: #2c3e50; color: #ffffff;">
                            <tr>
                                <th style="padding: 1rem; font-weight: 500;">Serial Number</th>
                                <th style="padding: 1rem; font-weight: 500;">Product Name</th>
                                <th style="padding: 1rem; font-weight: 500;">Count</th>
                            </tr>
                        </thead>
                        <tbody>
                            <% let serialNumber = 1; %>
                            <% bestSellingProducts.forEach(product => { %>
                                <tr style="transition: background-color 0.2s;" onmouseover="this.style.backgroundColor='#f5f7fa'" onmouseout="this.style.backgroundColor='#ffffff'">
                                    <td style="padding: 1rem; color: #34495e;"><%= serialNumber %></td>
                                    <td style="padding: 1rem; color: #34495e;"><%= product.name %></td>
                                    <td style="padding: 1rem; color: #34495e;"><%= product.count %></td>
                                </tr>
                                <% serialNumber++; %>
                            <% }); %>
                        </tbody>
                    </table>
                </div>
            </div>
            <div class="row m-3 my-5">
                <h4 class="text-start" style="font-size: 1.5rem; color: #2c3e50; font-weight: 600; margin-bottom: 1.5rem;">Best Selling Categories (Top 10)</h4>
                <div class="table-responsive">
                    <table class="table table-hover" style="background-color: #ffffff; border-radius: 8px; overflow: hidden; box-shadow: 0 4px 8px rgba(0,0,0,0.1);">
                        <thead style="background-color: #2c3e50; color: #ffffff;">
                            <tr>
                                <th style="padding: 1rem; font-weight: 500;">Serial Number</th>
                                <th style="padding: 1rem; font-weight: 500;">Category Name</th>
                                <th style="padding: 1rem; font-weight: 500;">Count</th>
                            </tr>
                        </thead>
                        <tbody>
                            <% let serialNumb = 1; %>
                            <% bestSellingCategories.forEach(category => { %>
                                <tr style="transition: background-color 0.2s;" onmouseover="this.style.backgroundColor='#f5f7fa'" onmouseout="this.style.backgroundColor='#ffffff'">
                                    <td style="padding: 1rem; color: #34495e;"><%= serialNumb %></td>
                                    <td style="padding: 1rem; color: #34495e;"><%= category.category_name %></td>
                                    <td style="padding: 1rem; color: #34495e;"><%= category.count %></td>
                                </tr>
                                <% serialNumb++; %>
                            <% }); %>
                        </tbody>
                    </table>
                </div>
            </div>
            <div class="row m-3 my-5">
                <h4 class="text-start" style="font-size: 1.5rem; color: #2c3e50; font-weight: 600; margin-bottom: 1.5rem;">Best Selling Brands (Top 10)</h4>
                <div class="table-responsive">
                    <table class="table table-hover" style="background-color: #ffffff; border-radius: 8px; overflow: hidden; box-shadow: 0 4px 8px rgba(0,0,0,0.1);">
                        <thead style="background-color: #2c3e50; color: #ffffff;">
                            <tr>
                                <th style="padding: 1rem; font-weight: 500;">Serial Number</th>
                                <th style="padding: 1rem; font-weight: 500;">Brand Name</th>
                                <th style="padding: 1rem; font-weight: 500;">Count</th>
                            </tr>
                        </thead>
                        <tbody>
                            <% let serialNum = 1; %>
                            <% bestSellingBrands.forEach(brand => { %>
                                <tr style="transition: background-color 0.2s;" onmouseover="this.style.backgroundColor='#f5f7fa'" onmouseout="this.style.backgroundColor='#ffffff'">
                                    <td style="padding: 1rem; color: #34495e;"><%= serialNum %></td>
                                    <td style="padding: 1rem; color: #34495e;"><%= brand.brand_name %></td>
                                    <td style="padding: 1rem; color: #34495e;"><%= brand.count %></td>
                                </tr>
                                <% serialNum++; %>
                            <% }); %>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </main>
</section>
        <footer class="main-footer font-xs">
            <div class="row pb-30 pt-15">
                <div class="col-sm-6">
                    <script>
                    document.write(new Date().getFullYear())
                    </script> ©, Evara - HTML Ecommerce Template .
                </div>
